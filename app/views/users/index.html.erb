<h1>All Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Profile</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= "#{user.first_name} #{user.last_name}" %></td>
        <td><%= link_to "View Profile", user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>